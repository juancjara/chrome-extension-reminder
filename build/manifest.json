{
  "manifest_version": 2,
  "name": "Reminder",
  "description": "set new description",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "omnibox": {"keyword": "reminder"},
  "icons": {
    "16": "/images/default-16.png"
  },
  "background": {
    "scripts": ["js/bg-bundle.js"]
  }
}
